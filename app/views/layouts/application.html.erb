</div>
  <nav class="cc-header-icons" data-ng-show="api.user.events.isAuthenticated">
    <!-- ngInclude: 'widgets/badges_popover.html' --><div class="cc-left ng-scope" data-ng-include="&#39;widgets/badges_popover.html&#39;"><div data-ng-controller="BadgesController" class="cc-launcher-icons ng-scope cc-badge-hasacademic" data-ng-class="{true:&#39;cc-badge-hasacademic&#39;}[studentInfo.regStatus.code != null &amp;&amp; api.user.profile.roles.student]">
  <!-- ngRepeat: badge in badges -->
  <div data-ng-repeat="badge in badges" class="cc-popover-container cc-popover-status cc-left ng-scope">
    <button class="cc-header-icon" data-ng-class="{true:&#39;cc-header-icon-selected&#39;}[api.popover.status(badge.cssPopover)]" data-ng-click="api.util.preventBubble($event);api.popover.toggle(badge.cssPopover)" data-ng-attr-title="{{badge.display.name}}" title="bMail">
      <div class="cc-ir cc-icon cc-icon-22 cc-left cc-icon-mail"></div>
      <span class="cc-visuallyhidden" data-ng-hide="api.popover.status(badge.cssPopover)">Show</span>
      <span class="cc-visuallyhidden ng-hide" data-ng-show="api.popover.status(badge.cssPopover)">Hide</span>
      <span class="cc-visuallyhidden ng-binding" data-ng-bind-template="your bMail">your bMail</span>
      <div class="cc-left cc-popover-count ng-binding" data-ng-hide="badge.display.name===&#39;bDrive&#39;" data-ng-bind="badge.count">324</div>
    </button>
  </div><!-- end ngRepeat: badge in badges -->
  <div data-ng-repeat="badge in badges" class="cc-popover-container cc-popover-status cc-left ng-scope">
    <button class="cc-header-icon" data-ng-class="{true:&#39;cc-header-icon-selected&#39;}[api.popover.status(badge.cssPopover)]" data-ng-click="api.util.preventBubble($event);api.popover.toggle(badge.cssPopover)" data-ng-attr-title="{{badge.display.name}}" title="bCal">
      <div class="cc-ir cc-icon cc-icon-22 cc-left cc-icon-calendar"></div>
      <span class="cc-visuallyhidden" data-ng-hide="api.popover.status(badge.cssPopover)">Show</span>
      <span class="cc-visuallyhidden ng-hide" data-ng-show="api.popover.status(badge.cssPopover)">Hide</span>
      <span class="cc-visuallyhidden ng-binding" data-ng-bind-template="your bCal">your bCal</span>
      <div class="cc-left cc-popover-count ng-binding" data-ng-hide="badge.display.name===&#39;bDrive&#39;" data-ng-bind="badge.count">0</div>
    </button>
  </div><!-- end ngRepeat: badge in badges -->
  <div data-ng-repeat="badge in badges" class="cc-popover-container cc-popover-status cc-left ng-scope">
    <button class="cc-header-icon" data-ng-class="{true:&#39;cc-header-icon-selected&#39;}[api.popover.status(badge.cssPopover)]" data-ng-click="api.util.preventBubble($event);api.popover.toggle(badge.cssPopover)" data-ng-attr-title="{{badge.display.name}}" title="bDrive">
      <div class="cc-ir cc-icon cc-icon-22 cc-left cc-icon-drive"></div>
      <span class="cc-visuallyhidden" data-ng-hide="api.popover.status(badge.cssPopover)">Show</span>
      <span class="cc-visuallyhidden ng-hide" data-ng-show="api.popover.status(badge.cssPopover)">Hide</span>
      <span class="cc-visuallyhidden ng-binding" data-ng-bind-template="your bDrive">your bDrive</span>
      <div class="cc-left cc-popover-count ng-binding ng-hide" data-ng-hide="badge.display.name===&#39;bDrive&#39;" data-ng-bind="badge.count">5</div>
    </button>
  </div><!-- end ngRepeat: badge in badges -->
</div>
</div>
    <!-- ngInclude: 'widgets/academic_profile_popover.html' -->
    <div class="cc-left ng-scope" data-ng-include="&#39;widgets/academic_profile_popover.html&#39;">
      <div data-ng-controller="StatusController" class="cc-popover-container cc-popover-status ng-scope" data-ng-show="studentInfo.regStatus.code != null &amp;&amp; api.user.profile.roles.student">
      <button class="cc-header-icon cc-header-icon-font cc-icon-status-person" data-ng-class="{&#39;cc-icon-status-person-haslayover&#39;:(hasAlerts),&#39;cc-header-icon-selected&#39;:(api.popover.status(&#39;cc-popover-academic-status&#39;))}" data-ng-click="api.util.preventBubble($event);api.popover.toggle(&#39;cc-popover-academic-status&#39;)" title="Show your status">
      <i class="fa fa-user cc-left"></i>
    <!-- ngIf: hasAlerts -->
    <!-- ngIf: !hasAlerts && hasWarnings -->
      </button>  
      </div>
    </div>
    <!-- setting button -->
    <div class="cc-left ng-scope" data-ng-include="&#39;widgets/gear_popover.html&#39;">
      <div class="cc-popover-container cc-popover-gear ng-scope">
      <button class="cc-header-icon cc-header-icon-font" data-ng-click="api.util.preventBubble($event);api.popover.toggle(&#39;cc-popover-menu&#39;)" data-ng-class="{true:&#39;cc-header-icon-selected&#39;}[api.popover.status(&#39;cc-popover-menu&#39;)]" title="Settings">
      <i class="fa fa-cog"></i>
      <span class="cc-visuallyhidden" data-ng-hide="api.popover.status(&#39;cc-popover-menu&#39;)">Show</span>
      <span class="cc-visuallyhidden ng-hide" data-ng-show="api.popover.status(&#39;cc-popover-menu&#39;)">Hide</span>
      <span class="cc-visuallyhidden">settings menu</span>
      </button>
      </div>
    </div> 

    <div class="cc-left ng-scope" data-ng-include="&#39;widgets/gear_popover.html&#39;">
      <div class="cc-popover-container cc-popover-gear ng-scope">
      <button class="cc-header-icon cc-header-icon-font" data-ng-click="api.util.preventBubble($event);api.popover.toggle(&#39;cc-popover-menu&#39;)" data-ng-class="{true:&#39;cc-header-icon-selected&#39;}[api.popover.status(&#39;cc-popover-menu&#39;)]" title="Sign In/Out">
      <% if current_user != nil %>
        <i class="fa fa-user"></i>
        <%= link_to current_user.full_name, edit_user_registration_path, class: "calcentral_header" %> |
        <%= link_to '<i class="fa fa-sign-out"></i>'.html_safe, destroy_user_session_path,  method: :delete, class: "calcentral_header", id: "Sign Out"%>
      <% else %>
        <%=link_to '<i class="fa fa-sign-in"></i>'.html_safe, new_user_session_path, class: "calcentral_header", id: "Sign In"%>
      <% end %>

      <span class="cc-visuallyhidden" data-ng-hide="api.popover.status(&#39;cc-popover-menu&#39;)">Show</span>
      <span class="cc-visuallyhidden ng-hide" data-ng-show="api.popover.status(&#39;cc-popover-menu&#39;)">Hide</span>
      <span class="cc-visuallyhidden">settings menu</span>
      </button>
      </div>
    </div> 


    <!-- 03/11, name/signin/signout button -->
    <div class="cc-left ng-scope" data-ng-include="&#39;widgets/gear_popover.html&#39;">
      <div class="cc-popover-container cc-popover-gear ng-scope">

      </div>
    </div> 
  </nav>
</header>
</div>

<div class="cc-off-canvas-container" data-ng-swipe-left="offCanvasMenu.show=false">
  <div class="cc-offcanvas-left cc-print-hide" id="cc-navigation">
    <!-- ngInclude: 'navigation.html' --><div data-ng-include="&#39;navigation.html&#39;" class="ng-scope"><!-- Top Navbar -->
<div class="row cc-navigation-bar ng-scope" data-ng-show="api.user.events.isAuthenticated">
