    <!-- ngIf: api.user.profile.hasAcademicsTab --><li data-ng-class="{true:&#39;active&#39;}[controllerName==&#39;AcademicsController&#39;]" data-ng-if="api.user.profile.hasAcademicsTab" class="ng-scope">
      <a href="https://calcentral.berkeley.edu/academics" data-ng-click="offCanvasMenu.show=false"><i class="fa fa-book"></i>My Academics</a>
    </li><!-- end ngIf: api.user.profile.hasAcademicsTab -->
    <!-- ngIf: api.user.profile.hasFinancialsTab --><li data-ng-class="{true:&#39;active&#39;}[controllerName==&#39;MyFinancesController&#39;]" data-ng-if="api.user.profile.hasFinancialsTab" class="ng-scope"><a href="https://calcentral.berkeley.edu/finances" data-ng-click="offCanvasMenu.show=false"><i class="fa fa-usd"></i>My Finances</a></li><!-- end ngIf: api.user.profile.hasFinancialsTab -->
    <li data-ng-class="{true:&#39;active&#39;}[controllerName==&#39;CampusController&#39;]"><a href="https://calcentral.berkeley.edu/campus" data-ng-click="offCanvasMenu.show=false"><i class="fa fa-th-large"></i>My Campus</a></li>

<!--
    roy incomplete
    <li data-ng-class="{true:&#39;active&#39;}[controllerName==&#39;CampusCOntroleer&#39;]">

-->
  </ul>
  <div class="cc-updated-feeds ng-scope" data-ng-controller="UpdatedFeedsController" data-ng-show="controllerName === &#39;DashboardController&#39;">
    <div data-ng-show="hasUpdates" class="">
      New data is available. <button class="cc-button" data-ng-click="refreshFeeds()">Load</button>
    </div>
  </div>
</div>
</div>
  </div>
  <div id="saas-content-body">
  <%= yield %>
  </div>
<!-- endinject -->
</body>
</html>
